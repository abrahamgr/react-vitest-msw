    "happy-dom": "^12.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint:fix"
  }
