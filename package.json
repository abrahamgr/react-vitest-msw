{
  "name": "react-vitest-msw",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "api": "tnode api/server.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:api": "echo 'Comming...'",
    "lint": "eslint src api --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
